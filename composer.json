{
	"name" : "phootwork/lang",
	"type" : "library",
	"description" : "Missing PHP language constructs",
	"authors" : [{
			"name" : "Thomas Gossmann",
			"homepage" : "http://gos.si"
		}
	],
	"license" : "MIT",
	"keywords" : [
		"array",
		"string",
		"comparison",
		"comparator"
	],
	"support" : {
		"issues" : "https://github.com/phootwork/lang/issues"
	},
	"autoload" : {
		"psr-4" : {
			"phootwork\\lang\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"phootwork\\lang\\tests\\" : "tests"
		}
	},
	"require" : {
		"php" : ">=5.6",
		"symfony/polyfill-mbstring" : "~1.8"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^5.7",
		"sami/sami" : "^4.1.0"
	},
	"scripts" : {
		"test" : "phpunit",
		"api" : "sami.php update sami.php"
	}
}